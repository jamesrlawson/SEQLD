w.abun = TRUE,  # use presence - absence converted data?\n             stand.x = FALSE,\n             corr = c(\"cailliez\"),\n             #                calc.FGR = TRUE, \n             #                clust.type = c(\"kmeans\"),\n             #                km.inf.gr = c(2),\n             #                km.sup.gr = c(10),\n             #                km.iter = (100),\n             #                calc.FDiv = TRUE, \n             #                calc.FRic = TRUE,\n             m = \"max\",\n             calc.CWM=TRUE, \n             print.pco=TRUE, \n             #                scale.RaoQ=TRUE, \n             #               stand.FRic=TRUE\n)\n",
